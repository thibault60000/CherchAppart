{% extends '@CherchAppart/Layout/base.html.twig' %}
{% block body %}
    <h1>Créer un compte</h1>

    {{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.password) }}
    {{ form_row(form.mail) }}
    {{ form_row(form.name) }}
    {{ form_row(form.lastname) }}
    {{ form_row(form.bornDate) }}
    {{ form_row(form.adress) }}
    {{ form_row(form.postalCode) }}
    {{ form_row(form.city) }}
    {{ form_row(form.sexe) }}
    {{ form_row(form.phone) }}
    {{ form_row(form.mobilePhone) }}
    <input type="submit" value="Créer" />
    {{ form_end(form) }}
{% endblock %}
